{
  "schemaVersion": 1,
  "id": "ait-extras",
  "version": "${version}",
  "name": "AIT Extras",
  "description": "Where all the stuff ait doesn't want will go, along with some other stuff",
  "authors": ["Addi3"],
  "license": "MIT",
  "icon": "assets/ait-extras/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.aitextras.datagen.AITExtrasDataGenerator"
    ],
    "client": [
      "com.aitextras.client.AITExtrasClient"
    ],
    "main": [
      "com.aitextras.AITExtras"
    ],
    "ait-main": [
      "com.aitextras.ait.AITExtrasAddon"
    ]
  },
  "mixins": [
    "ait-extras.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "ait": ">=1.1.0",
    "amblekit": ">=1.1.3-1.20.1-beta"
  }
}
